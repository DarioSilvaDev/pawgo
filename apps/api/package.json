{
  "name": "@pawgo/api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "worker:dev": "tsx watch src/worker.ts",
    "worker:start": "node dist/worker.js",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prisma:generate": "prisma generate",
    "prisma:fix-client": "node scripts/fix-prisma-client.mjs",
    "prisma:generate:full": "npm run prisma:generate && npm run prisma:fix-client",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "postinstall": "npm run prisma:fix-client"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.490.0",
    "@aws-sdk/s3-request-presigner": "3.490.0",
    "fastify": "4.29.1",
    "@fastify/cors": "8.4.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/jwt": "9.0.0",
    "@fastify/multipart": "8.3.1",
    "@fastify/static": "6.12.0",
    "@prisma/client": "5.22.0",
    "@types/bcrypt": "6.0.0",
    "@types/luxon": "3.7.1",
    "@types/node": "22.15.0",
    "@types/nodemailer": "7.0.4",
    "@types/qrcode": "1.5.6",
    "bcrypt": "6.0.0",
    "dotenv": "16.6.1",
    "luxon": "3.7.2",
    "mercadopago": "2.11.0",
    "nanoid": "5.1.6",
    "nodemailer": "7.0.12",
    "pg-boss": "12.5.4",
    "qrcode": "1.5.4",
    "resend": "6.9.2",
    "speakeasy": "2.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "prisma": "5.22.0",
    "typescript": "5.3.3",
    "tsx": "4.7.0",
    "eslint": "8.57.0",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.13.2"
  }
}